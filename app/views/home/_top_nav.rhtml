			&nbsp;&nbsp;&nbsp;<%= link_to "logout", :controller => "account", :action => "logout" %>
		<% end-%>
	<% else %>
		&nbsp;&nbsp;&nbsp;<%= link_to "login", :controller => "account", :action => "login" %> 
		or <%= link_to "sign up", :controller => "account", :action => "signup" %>
