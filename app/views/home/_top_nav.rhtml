	&nbsp;<%= link_to "playlist", :controller => "recommendation", :action => "playlist" %>
	&nbsp;<%= link_to "automatron", :controller => "recommendation", :action => "automatron" %>
	<% if logged_in? %>
		<% if current_user.fake == 1 %>
			&nbsp;&nbsp;&nbsp;<%= link_to "claim user", :controller => "account", :action => "claim" %> or <%= link_to "login other user", :controller => "account", :action => "login" %> 
		<% end %>
